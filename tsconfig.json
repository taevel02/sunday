{
  "compilerOptions": {
    "incremental": true,
    "target": "esnext",
    "module": "NodeNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "preserveValueImports": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true
  }
}
