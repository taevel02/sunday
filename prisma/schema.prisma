generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model PreferredShares {
  id                 Int    @id @default(autoincrement())
  ISU_CD             String
  ISU_ABBRV          String
  MKT_ID             String
  MKT_NM             String
  KIND_STKCERT_TP_NM String
  LIST_DD            String
}

model CustomExceptionShares {
  id        Int    @id @default(autoincrement())
  ISU_CD    String
  ISU_ABBRV String
  MKT_ID    String
  MKT_NM    String
}

model SearchCondition {
  name      String  @id
  isEnabled Boolean
}

model YouthHousing {
  id      Int    @id @default(autoincrement())
  boardId Int
  nttSj   String
  optn1   String
  optn2   String
  optn3   String
  optn4   String
  regDate BigInt
  url     String
}
